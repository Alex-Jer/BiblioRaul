{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "moment": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-unused-vars": "warn",
    "space-before-function-paren": "off"
  }
}
